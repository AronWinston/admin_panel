<h1>All Cohorts</h1>


<ul class="list">
<% @cohorts.each do |cohort|%>
    <p>Name: <%=cohort.name%> <%= link_to "Edit Cohort", edit_cohort_path(cohort) %> <%= link_to "Destroy Cohort", cohort_path(cohort), method: :delete %></p>
        <li>Start date: <%=cohort.startdate%></li>
        <li>End date: <%=cohort.enddate%></li>
        <li>Students: <%=cohort.students%></li>
        <li>Course Id: <%=cohort.course.course_name%></li>
    <br>
    
<%end%>
</ul>